{

    "manifest_version": 3,
    "name": "Browsing WatchDog",
    "version": "1.0",
  
    "description": "Helps you to streamline your browsing habits.",
  
    "icons": {
      "48": "icons/watchdog-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content-script.js"],
        "css" : ["content-style.css"]
      }
    ]
  
  }